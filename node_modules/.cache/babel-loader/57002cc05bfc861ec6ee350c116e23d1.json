{"ast":null,"code":"import { useRoute } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    const route = useRoute();\n    const todo = ref(null);\n    const loading = ref(true);\n\n    const getTodo = async () => {\n      const res = await axios.get('http://localhost:3000/todos/' + route.params.id);\n      todo.value = res.data;\n      loading.value = false;\n    };\n\n    const toggleTodoStatus = () => {\n      todo.completed = !todo.completed;\n    };\n\n    getTodo();\n    return {\n      todo,\n      loading,\n      toggleTodoStatus\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqCA,SAAQA,QAAR,QAAuB,YAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH,UAAMC,KAAI,GAAIH,QAAQ,EAAtB;AACA,UAAMI,IAAG,GAAIC,GAAG,CAAC,IAAD,CAAhB;AACA,UAAMC,OAAM,GAAID,GAAG,CAAC,IAAD,CAAnB;;AAGA,UAAME,OAAM,GAAI,YAAW;AACvB,YAAMC,GAAE,GAAI,MAAMP,KAAK,CAACQ,GAAN,CAAU,iCAAiCN,KAAK,CAACO,MAAN,CAAaC,EAAxD,CAAlB;AAEAP,UAAI,CAACQ,KAAL,GAAaJ,GAAG,CAACK,IAAjB;AACAP,aAAO,CAACM,KAAR,GAAgB,KAAhB;AAEJ,KANA;;AAOA,UAAME,gBAAe,GAAI,MAAM;AAC3BV,UAAI,CAACW,SAAL,GAAiB,CAACX,IAAI,CAACW,SAAvB;AACJ,KAFA;;AAIAR,WAAO;AACP,WAAO;AACHH,UADG;AAEHE,aAFG;AAGHQ;AAHG,KAAP;AAKJ;;AAxBW,CAAf","names":["useRoute","axios","setup","route","todo","ref","loading","getTodo","res","get","params","id","value","data","toggleTodoStatus","completed"],"sourceRoot":"","sources":["E:\\vue\\vue_todo\\src\\pages\\todos\\_id.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <h1>Todo Page</h1>\r\n        <div v-if=\"loading\">\r\n            Loading...\r\n        </div>\r\n        <form v-else>\r\n            <div class=\"row\">\r\n                <div class=\"col-6\">\r\n                     <div class=\"form-group\">\r\n                        <label for=\"\">Subject</label>\r\n                        <input v-model=\"todo.subject\" type=\"text\" class=\"form-control\">\r\n\r\n                    </div>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                     <div class=\"form-group\">\r\n                        <label for=\"\">Status</label>\r\n                            <div>\r\n                                <button class=\"btn \"\r\n                                :class= \"todo.completed ? 'btn-success' : 'btn-danger'\"\r\n                                @click.self.prevent=\"toggleTodoStatus\"> \r\n                                {{ todo.completed ? 'Completed' : 'Incomplete'}}\r\n                                \r\n                                </button>\r\n                           \r\n                            </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n           \r\n            <button class=\"btn btn-primary\">저장</button>\r\n        </form>\r\n\r\n    </div>\r\n</template>\r\n<script>\r\nimport {useRoute} from 'vue-router'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    setup(){\r\n        const route = useRoute();\r\n        const todo = ref(null);\r\n        const loading = ref(true);\r\n\r\n\r\n        const getTodo = async() => {\r\n            const res = await axios.get('http://localhost:3000/todos/' + route.params.id)\r\n        \r\n            todo.value = res.data; \r\n            loading.value = false;\r\n\r\n        }\r\n        const toggleTodoStatus = () => {\r\n            todo.completed = !todo.completed\r\n        }\r\n\r\n        getTodo();\r\n        return {\r\n            todo,\r\n            loading,\r\n            toggleTodoStatus\r\n        }\r\n    }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}