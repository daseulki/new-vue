{"ast":null,"code":"import { ref, computed } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nexport default {\n  name: 'App',\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n\n    const addTodo = todo => {\n      todos.value.push(todo);\n    };\n\n    const deleteTodo = index => {\n      todos.value.splice(index, 1);\n    };\n\n    const toggleTodo = index => {\n      console.log(index);\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if (searchText.value) {\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      }\n\n      return todos.value;\n    });\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoBA,SAASA,GAAT,EAAcC,QAAd,QAA8B,KAA9B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AAEA,eAAe;AACbC,MAAI,EAAE,KADO;AAEbC,YAAU,EAAE;AACVH,kBADU;AAEVC;AAFU,GAFC;;AAObG,OAAK,GAAE;AACL,UAAMC,KAAI,GAAIP,GAAG,CAAC,EAAD,CAAjB;;AACA,UAAMQ,OAAM,GAAKC,IAAD,IAAU;AACxBF,WAAK,CAACG,KAAN,CAAYC,IAAZ,CAAiBF,IAAjB;AACF,KAFA;;AAGA,UAAMG,UAAS,GAAKC,KAAD,IAAW;AAC5BN,WAAK,CAACG,KAAN,CAAYI,MAAZ,CAAmBD,KAAnB,EAA0B,CAA1B;AACF,KAFA;;AAGA,UAAME,UAAS,GAAKF,KAAD,IAAW;AAC5BG,aAAO,CAACC,GAAR,CAAYJ,KAAZ;AACAN,WAAK,CAACG,KAAN,CAAYG,KAAZ,EAAmBK,SAAnB,GAA+B,CAACX,KAAK,CAACG,KAAN,CAAYG,KAAZ,EAAmBK,SAAnD;AACF,KAHA;;AAIA,UAAMC,UAAS,GAAInB,GAAG,CAAC,EAAD,CAAtB;AACA,UAAMoB,aAAY,GAAInB,QAAQ,CAAC,MAAI;AACjC,UAAGkB,UAAU,CAACT,KAAd,EAAoB;AAClB,eAAOH,KAAK,CAACG,KAAN,CAAYW,MAAZ,CAAmBZ,IAAI,IAAE;AAC9B,iBAAOA,IAAI,CAACa,OAAL,CAAaC,QAAb,CAAsBJ,UAAU,CAACT,KAAjC,CAAP;AACD,SAFM,CAAP;AAGF;;AACA,aAAOH,KAAK,CAACG,KAAb;AACD,KAP6B,CAA9B;AASA,WAAO;AACLH,WADK;AAELC,aAFK;AAGLI,gBAHK;AAILG,gBAJK;AAKLI,gBALK;AAMLC;AANK,KAAP;AAQD;;AArCY,CAAf","names":["ref","computed","TodoSimpleForm","TodoList","name","components","setup","todos","addTodo","todo","value","push","deleteTodo","index","splice","toggleTodo","console","log","completed","searchText","filteredTodos","filter","subject","includes"],"sourceRoot":"","sources":["E:\\vue\\vue_todo\\src\\App.vue"],"sourcesContent":["<template>\n\n<div class=\"container\">\n  <h2>To-Do List</h2>\n  <input type=\"text\" class=\"form-control\" \n  v-model=\"searchText\" placeholder=\"Search\">\n  <hr>\n  <TodoSimpleForm @add-todo = \"addTodo\" ></TodoSimpleForm>\n  <div v-if=\"!filteredTodos.length\"> Nothing </div>\n  <TodoList \n    :todos = \"filteredTodos\"\n    @toggle-todo=\"toggleTodo\"\n    @delete-todo=\"deleteTodo\"></TodoList>\n\n</div>\n\n</template>\n\n<script>\n\nimport { ref, computed } from 'vue'\nimport TodoSimpleForm from './components/TodoSimpleForm.vue'\nimport TodoList from './components/TodoList.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    TodoSimpleForm,\n    TodoList\n    \n  },\n  setup(){\n    const todos = ref([]);\n    const addTodo = (todo) => { \n      todos.value.push(todo);\n    }\n    const deleteTodo = (index) => {\n      todos.value.splice(index, 1);\n    }\n    const toggleTodo = (index) => {\n      console.log(index);\n      todos.value[index].completed = !todos.value[index].completed\n    }\n    const searchText = ref('');\n    const filteredTodos = computed(()=>{\n      if(searchText.value){\n        return todos.value.filter(todo=>{\n          return todo.subject.includes(searchText.value)\n        });\n      }\n      return todos.value;\n    })\n\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n    }\n  },\n\n}\n</script>\n\n<style>\n</style>\n"]},"metadata":{},"sourceType":"module"}