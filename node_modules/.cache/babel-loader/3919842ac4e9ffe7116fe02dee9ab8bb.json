{"ast":null,"code":"import { ref, computed, watchEffect } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\nexport default {\n  name: 'App',\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n    const numOfTodos = ref(0);\n    const limit = 5;\n    const curPage = ref(1);\n    watchEffect(() => {\n      console.log(curPage.value);\n    });\n    const numOfPages = computed(() => {\n      return Math.ceil(numOfTodos.value / limit);\n    });\n\n    const getTodo = async (page = curPage.value) => {\n      try {\n        const res = await axios.get(`http://localhost:3000/todos?_page=${page}&_limit=${limit}`);\n        todos.value = res.data;\n        numOfTodos.value = res.headers['x-total-count'];\n      } catch (err) {\n        console.error(err);\n      }\n    };\n\n    getTodo();\n\n    const addTodo = async todo => {\n      try {\n        const res = await axios.post('http://localhost:3000/todos', {\n          subject: todo.subject,\n          completed: todo.completed\n        });\n        todos.value.push(res.data);\n      } catch (err) {\n        console.error(err);\n      }\n    };\n\n    const deleteTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.delete('http://localhost:3000/todos/' + id);\n        todos.value.splice(index, 1);\n        getTodo();\n      } catch (err) {\n        console.error(err);\n      }\n    };\n\n    const toggleTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.patch('http://localhost:3000/todos/' + id, {\n          completed: !todos.value[index].completed\n        });\n        todos.value[index].completed = !todos.value[index].completed;\n      } catch (err) {\n        console.error(err);\n      }\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if (searchText.value) {\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      }\n\n      return todos.value;\n    });\n    return {\n      todos,\n      getTodo,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n      numOfPages,\n      curPage\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6BE,SACEA,GADF,EAEEC,QAFF,EAGEC,WAHF,QAIO,KAJP;AAKA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,KADO;AAEbC,YAAU,EAAE;AACVJ,kBADU;AAEVC;AAFU,GAFC;;AAObI,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIT,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMU,UAAS,GAAIV,GAAG,CAAC,CAAD,CAAtB;AACA,UAAMW,KAAI,GAAI,CAAd;AACA,UAAMC,OAAM,GAAIZ,GAAG,CAAC,CAAD,CAAnB;AAEAE,eAAU,CAAG,MAAM;AACjBW,aAAO,CAACC,GAAR,CAAYF,OAAO,CAACG,KAApB;AAGD,KAJS,CAAV;AAMA,UAAMC,UAAS,GAAIf,QAAQ,CAAC,MAAI;AAC9B,aAAOgB,IAAI,CAACC,IAAL,CAAUR,UAAU,CAACK,KAAX,GAAiBJ,KAA3B,CAAP;AACD,KAF0B,CAA3B;;AAIA,UAAMQ,OAAM,GAAI,OAAOC,IAAG,GAAIR,OAAO,CAACG,KAAtB,KAAgC;AAC9C,UAAI;AACF,cAAMM,GAAE,GAAI,MAAMhB,KAAK,CAACiB,GAAN,CAAW,qCAAoCF,IAAK,WAAUT,KAAM,EAApE,CAAlB;AACAF,aAAK,CAACM,KAAN,GAAcM,GAAG,CAACE,IAAlB;AACAb,kBAAU,CAACK,KAAX,GAAmBM,GAAG,CAACG,OAAJ,CAAY,eAAZ,CAAnB;AAGF,OANA,CAME,OAAOC,GAAP,EAAY;AACZZ,eAAO,CAACa,KAAR,CAAcD,GAAd;AACF;AACF,KAVA;;AAWAN,WAAO;;AAEP,UAAMQ,OAAM,GAAI,MAAOC,IAAP,IAAgB;AAC9B,UAAI;AACF,cAAMP,GAAE,GAAI,MAAMhB,KAAK,CAACwB,IAAN,CAAW,6BAAX,EAA0C;AAC1DC,iBAAO,EAAEF,IAAI,CAACE,OAD4C;AAE1DC,mBAAS,EAAEH,IAAI,CAACG;AAF0C,SAA1C,CAAlB;AAIAtB,aAAK,CAACM,KAAN,CAAYiB,IAAZ,CAAiBX,GAAG,CAACE,IAArB;AACF,OANA,CAME,OAAOE,GAAP,EAAY;AACZZ,eAAO,CAACa,KAAR,CAAcD,GAAd;AACF;AAEF,KAXA;;AAYA,UAAMQ,UAAS,GAAI,MAAOC,KAAP,IAAiB;AAClC,YAAMC,EAAC,GAAI1B,KAAK,CAACM,KAAN,CAAYmB,KAAZ,EAAmBC,EAA9B;;AACA,UAAI;AACF,cAAM9B,KAAK,CAAC+B,MAAN,CAAa,iCAAiCD,EAA9C,CAAN;AACA1B,aAAK,CAACM,KAAN,CAAYsB,MAAZ,CAAmBH,KAAnB,EAA0B,CAA1B;AACAf,eAAO;AAET,OALA,CAKE,OAAOM,GAAP,EAAY;AACZZ,eAAO,CAACa,KAAR,CAAcD,GAAd;AACF;AAEF,KAXA;;AAaA,UAAMa,UAAS,GAAI,MAAOJ,KAAP,IAAiB;AAClC,YAAMC,EAAC,GAAI1B,KAAK,CAACM,KAAN,CAAYmB,KAAZ,EAAmBC,EAA9B;;AACA,UAAI;AACF,cAAM9B,KAAK,CAACkC,KAAN,CAAY,iCAAiCJ,EAA7C,EAAiD;AACrDJ,mBAAS,EAAE,CAACtB,KAAK,CAACM,KAAN,CAAYmB,KAAZ,EAAmBH;AADsB,SAAjD,CAAN;AAGAtB,aAAK,CAACM,KAAN,CAAYmB,KAAZ,EAAmBH,SAAnB,GAA+B,CAACtB,KAAK,CAACM,KAAN,CAAYmB,KAAZ,EAAmBH,SAAnD;AAEF,OANA,CAME,OAAON,GAAP,EAAY;AACZZ,eAAO,CAACa,KAAR,CAAcD,GAAd;AACF;AACF,KAXA;;AAaA,UAAMe,UAAS,GAAIxC,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMyC,aAAY,GAAIxC,QAAQ,CAAC,MAAM;AACnC,UAAIuC,UAAU,CAACzB,KAAf,EAAsB;AACpB,eAAON,KAAK,CAACM,KAAN,CAAY2B,MAAZ,CAAmBd,IAAG,IAAK;AAChC,iBAAOA,IAAI,CAACE,OAAL,CAAaa,QAAb,CAAsBH,UAAU,CAACzB,KAAjC,CAAP;AACD,SAFM,CAAP;AAGF;;AACA,aAAON,KAAK,CAACM,KAAb;AACD,KAP6B,CAA9B;AASA,WAAO;AACLN,WADK;AAELU,aAFK;AAGLQ,aAHK;AAILM,gBAJK;AAKLK,gBALK;AAMLE,gBANK;AAOLC,mBAPK;AAQLzB,gBARK;AASLJ;AATK,KAAP;AAWD;;AAhGY,CAAf","names":["ref","computed","watchEffect","TodoSimpleForm","TodoList","axios","name","components","setup","todos","numOfTodos","limit","curPage","console","log","value","numOfPages","Math","ceil","getTodo","page","res","get","data","headers","err","error","addTodo","todo","post","subject","completed","push","deleteTodo","index","id","delete","splice","toggleTodo","patch","searchText","filteredTodos","filter","includes"],"sourceRoot":"","sources":["E:\\vue\\vue_todo\\src\\App.vue"],"sourcesContent":["<template>\n\n  <div class=\"container\">\n    <h2 class=\"strong\">To-Do List</h2>\n    <input type=\"text\" class=\"form-control\" v-model=\"searchText\" placeholder=\"Search\">\n    <hr>\n    <TodoSimpleForm @add-todo=\"addTodo\"></TodoSimpleForm>\n    <div v-if=\"!filteredTodos.length\"> Nothing </div>\n    <TodoList :todos=\"filteredTodos\" @toggle-todo=\"toggleTodo\" @delete-todo=\"deleteTodo\"></TodoList>\n\n    <hr>\n    <nav aria-label=\"Page navigation example\">\n      <ul class=\"pagination\">\n        <li class=\"page-item\" @click='getTodo(curPage-1);curPage = curPage-1'>\n          <a class=\"page-link\" >Previous</a></li>\n       \n        <li v-for=\"page in numOfPages\" \n        :key=\"page\" :class=\"curPage === page? 'active':''\"\n        @click='getTodo(page); curPage = page'\n        class=\"page-item\"><a class=\"page-link\" href=\"#\">{{ page }}</a></li>\n\n        <li class=\"page-item\" @click='getTodo(curPage+1); curPage = curPage+1'>\n          <a class=\"page-link\" >Next</a></li>\n      </ul>\n    </nav>\n  </div>\n</template>\n\n<script>\n  import {\n    ref,\n    computed,\n    watchEffect\n  } from 'vue'\n  import TodoSimpleForm from './components/TodoSimpleForm.vue'\n  import TodoList from './components/TodoList.vue'\n  import axios from 'axios'\n\n  export default {\n    name: 'App',\n    components: {\n      TodoSimpleForm,\n      TodoList\n\n    },\n    setup() {\n      const todos = ref([]);\n      const numOfTodos = ref(0);\n      const limit = 5;\n      const curPage = ref(1);\n\n      watchEffect (() => {\n        console.log(curPage.value)\n\n\n      })\n\n      const numOfPages = computed(()=>{\n        return Math.ceil(numOfTodos.value/limit)\n      })\n\n      const getTodo = async (page = curPage.value) => {\n        try {\n          const res = await axios.get(`http://localhost:3000/todos?_page=${page}&_limit=${limit}`)\n          todos.value = res.data;\n          numOfTodos.value = res.headers['x-total-count'];\n\n          \n        } catch (err) {\n          console.error(err)\n        }\n      }\n      getTodo();\n\n      const addTodo = async (todo) => {\n        try {\n          const res = await axios.post('http://localhost:3000/todos', {\n            subject: todo.subject,\n            completed: todo.completed,\n          })\n          todos.value.push(res.data);\n        } catch (err) {\n          console.error(err)\n        }\n\n      }\n      const deleteTodo = async (index) => {\n        const id = todos.value[index].id;\n        try {\n          await axios.delete('http://localhost:3000/todos/' + id)\n          todos.value.splice(index, 1);\n          getTodo();\n\n        } catch (err) {\n          console.error(err)\n        }\n\n      }\n\n      const toggleTodo = async (index) => {\n        const id = todos.value[index].id;\n        try {\n          await axios.patch('http://localhost:3000/todos/' + id, {\n            completed: !todos.value[index].completed\n          })\n          todos.value[index].completed = !todos.value[index].completed\n\n        } catch (err) {\n          console.error(err)\n        }\n      }\n\n      const searchText = ref('');\n\n      const filteredTodos = computed(() => {\n        if (searchText.value) {\n          return todos.value.filter(todo => {\n            return todo.subject.includes(searchText.value)\n          });\n        }\n        return todos.value;\n      })\n\n      return {\n        todos,\n        getTodo,\n        addTodo,\n        deleteTodo,\n        toggleTodo,\n        searchText,\n        filteredTodos,\n        numOfPages,\n        curPage,\n      }\n    },\n  }\n</script>\n\n<style scoped>\nnav a{\n  cursor:pointer;\n}\n</style>"]},"metadata":{},"sourceType":"module"}